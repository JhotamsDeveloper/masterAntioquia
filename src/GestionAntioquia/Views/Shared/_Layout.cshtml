
<!DOCTYPE html>
<html lang="es-co">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] vive la historias de los pueblos</title>

    <link rel="icon" type="image/png" sizes="32x32" href="~/images/favicon.png">

    <link rel="stylesheet" type="text/css" href="~/css/bootstrap-4.1.2/bootstrap.min.css">

    <link href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.2.0/css/all.css" integrity="sha384-hWVjflwFxL6sNzntih27bfxkr27PmbbK/iSvJ+a4+0owXq79v+lsFkW54bOGbiDQ" crossorigin="anonymous">

    <link rel="stylesheet" type="text/css" href="~/lib/OwlCarousel2-2.3.4/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="~/lib/OwlCarousel2-2.3.4/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="~/lib/OwlCarousel2-2.3.4/animate.css">
    <link rel="stylesheet" type="text/css" href="~/lib/colorbox/colorbox.css">

    <link rel="stylesheet" type="text/css" href="~/css/bootstrap-datepicker.css">
    <link rel="stylesheet" type="text/css" href="~/css/jquery.timepicker.css">


    <link rel="stylesheet" type="text/css" href="~/css/main_styles.css">
    <link rel="stylesheet" type="text/css" href="~/css/responsive.css">

    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>

    <!-- Menu -->
    <div class="menu">
        <div class="menu_container text-right">
            <div class="menu_close">close</div>
            <nav class="menu_nav">
                <ul>
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                    <li><a asp-area="" asp-controller="Home" asp-action="About">¿Quiénes somos?</a></li>
                    <li><a href="/souvenir">Empresas</a></li>
                    <li><a href="/tienda-virtual">Tienda Virtual</a></li>
                    <li><a href="/blog">Blog</a></li>
                </ul>
            </nav>
            <div class="menu_link">
                <a asp-area="" asp-controller="Home" asp-action="Contact">Contáctenos</a>
            </div>
        </div>
    </div>

    <div class="super_container">
        <!-- Header -->

        <header class="header">
            @*<div class="header_background">
                    <div class="background_image"
                         style="background-image: url(../images/header.jpg);"></div>
                </div>*@
            <div class="header_overlay"></div>
            <div class="header_content d-flex flex-row align-items-center justify-content-start header_JhotaMS">
                <!-- Logo -->
                <div class="logo">
                    @*<a href="#">Pueblos.<span>Travel</span></a>*@
                    <a asp-controller="Home" asp-action="Index"><img src="~/images/logo-pueblo-travel.svg" alt="Pueblos Travel" width="100"></a>
                </div>

                <!-- Header Nav -->
                <div class="header_right d-flex flex-row align-items-center justify-content-start ml-auto">
                    <nav class="main_nav">
                        <ul class="d-flex flex-row align-items-center justify-content-start">

                            <li><a asp-area="" asp-controller="Home" asp-action="Index">Inicio</a></li>
                            <li><a asp-area="" asp-controller="Home" asp-action="About">¿Quiénes somos?</a></li>
                            <li><a href="/souvenir">Empresas</a></li>
                            <li><a href="/tienda-virtual">Tienda Virtual</a></li>
                            <li><a href="/blog">Blog</a></li>
                        </ul>
                    </nav>
                    <div class="add_listing text-center trans_200">
                        <a asp-area="" asp-controller="Home" asp-action="Contact">Contáctenos</a>
                    </div>

                    <partial name="_LoginPartial" />

                    <div class="hamburger">
                        <i class="fa fa-bars trans_200"></i>
                    </div>
                </div>
            </div>
        </header>
        <div class="super_container_inner">
            <div class="super_overlay"></div>
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
            <!--footer starts from here-->
            <footer class="footer">

                <div class="container">

                    <ul class="social_footer_ul">
                        <li><a href="http://webenlance.com"><i class="fab fa-facebook-f"></i></a></li>
                        <li><a href="http://webenlance.com"><i class="fab fa-twitter"></i></a></li>
                        <li><a href="http://webenlance.com"><i class="fab fa-linkedin"></i></a></li>
                        <li><a href="http://webenlance.com"><i class="fab fa-instagram"></i></a></li>
                    </ul>
                    <br />
                    <p class="text-center">Copyright 2020 - @DateTime.Now.Year | Diseñado con por <a href="https://www.linkedin.com/in/jhotams/" target="_blank"><span>JhotaMS.Developer</span></a></p>

                </div>

            </footer>
        </div>
    </div>
    @*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@
    <script src="~/js/jquery-3.3.1.min.js"></script>
    <script src="~/css/bootstrap-4.1.2/popper.js"></script>
    <script src="~/css/bootstrap-4.1.2/bootstrap.min.js"></script>

    @*www.tiny.cloud/*@
    <script src="https://cdn.tiny.cloud/1/fx3uf5far79v8h24dqwntqiaf5kq03fcderm49440staohhq/tinymce/5/tinymce.min.js" referrerpolicy="origin"></script>

    <script src="~/lib/greensock/TweenMax.min.js"></script>
    <script src="~/lib/greensock/TimelineMax.min.js"></script>

    <script src="~/lib/scrollmagic/ScrollMagic.min.js"></script>
    <script src="~/lib/greensock/animation.gsap.min.js"></script>
    <script src="~/lib/greensock/ScrollToPlugin.min.js"></script>

    <script src="~/lib/Isotope/isotope.pkgd.min.js"></script>
    <script src="~/lib/easing/easing.js"></script>
    <script src="~/lib/progressbar/progressbar.min.js"></script>
    <script src="~/lib/parallax-js-master/parallax.min.js"></script>
    <script src="~/lib/colorbox/jquery.colorbox-min.js"></script>

    <script src="~/js/site.js" asp-append-version="true"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
