
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] se parte de la Ciudad</title>

    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,700,900" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="~/css/bootstrap-4.1.2/bootstrap.min.css">
    <link href="~/lib/font-awesome-4.7.0/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    <link href="~/lib/colorbox/colorbox.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="~/css/listing.css">
    <link rel="stylesheet" type="text/css" href="~/css/listings_responsive.css">

    <link rel="stylesheet" href="~/css/site.css" />

</head>
<body>


    <!-- Menu -->

    <div class="menu">
      <div class="menu_container text-right">
        <div class="menu_close">close</div>
        <nav class="menu_nav">
          <ul>
            <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Privacy">About us</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Index">Listings</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Index">News</a></li>
            <li><a asp-area="" asp-controller="Home" asp-action="Index">Contact</a></li>
          </ul>
        </nav>
        <div class="menu_link">
          <a href="#">Contáctenos</a>
        </div>
      </div>
    </div>

    <div class="super_container">
      <!-- Header -->

      <header class="header">
        <div class="header_background">
          <div
            class="background_image"
            style="background-image: url(images/header.jpg);"
          ></div>
        </div>
        <div class="header_overlay"></div>
        <div
          class="header_content d-flex flex-row align-items-center justify-content-start"
        >
          <!-- Logo -->
          <div class="logo">
              <a href="#">JhotaMS.<span>Developer</span><span>+</span></a>
          </div>

          <!-- Header Nav -->
          <div class="header_right d-flex flex-row align-items-center justify-content-start ml-auto">
              <nav class="main_nav">
                  <ul class="d-flex flex-row align-items-center justify-content-start">

                      <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                      <li><a asp-area="" asp-controller="Home" asp-action="Privacy">About us</a></li>
                      <li><a asp-area="" asp-controller="Home" asp-action="Index">Listings</a></li>
                      <li><a asp-area="" asp-controller="Home" asp-action="Index">News</a></li>
                      <li><a asp-area="" asp-controller="Home" asp-action="Index">Contact</a></li>
                  </ul>
              </nav>
              <div class="add_listing text-center trans_200">
                  <a href="#">Contáctenos</a>
              </div>

              <partial name="_LoginPartial" />

              @*<div class="log_reg">
                  <ul class="d-flex flex-row align-items-center justify-content-start">
                      <li><a href="#">Login</a></li>
                      <li><a href="#">Register</a></li>
                  </ul>
              </div>*@

              <div class="hamburger">
                  <i class="fa fa-bars trans_200"></i>
              </div>
          </div>
        </div>
      </header>

        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <footer class="border-top footer text-muted">
            <div class="container">
                &copy; 2020 - GestionAntioquia - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
            </div>
        </footer>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/css/bootstrap-4.1.2/popper.js"></script>
    <script src="~/css/bootstrap-4.1.2/bootstrap.min.js"></script>
    <script src="~/lib/greensock/TweenMax.min.js"></script>
    <script src="~/lib/greensock/TimelineMax.min.js"></script>
    <script src="~/lib/scrollmagic/ScrollMagic.min.js"></script>
    <script src="~/lib/greensock/animation.gsap.min.js"></script>
    <script src="~/lib/greensock/ScrollToPlugin.min.js"></script>
    <script src="~/lib/easing/easing.js"></script>
    <script src="~/lib/progressbar/progressbar.min.js"></script>
    <script src="~/lib/parallax-js-master/parallax.min.js"></script>
    <script src="~/lib/colorbox/jquery.colorbox-min.js"></script>

    @*<script src="https://maps.googleapis.com/maps/api/js?v=3.exp&key=AIzaSyCIwF204lFZg1y4kPSIhKaHEXMLYxxuMhA"></script>*@
    <script src="~/js/listing.js" asp-append-version="true"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>


    @RenderSection("Scripts", required: false)
</body>
</html>
