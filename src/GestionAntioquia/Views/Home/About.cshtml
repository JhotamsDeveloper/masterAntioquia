@{
    ViewData["Title"] = "¿Quiénes Somos?";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewData["imagen"] = Url.Content("~/images/blog.jpg");
}

<div class="super_overlay"></div>

<!-- Home -->

<div class="home">
    <div class="parallax_background parallax-window" data-parallax="scroll" data-image-src="@ViewData["imagen"]" data-speed="0.8"></div>
    <div class="home_container">
        <div class="container">
            <div class="row">
                <div class="col">
                    <div class="home_content">
                        <div class="home_title">@ViewData["Title"]</div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="container m-5">
    <div class="section_title"><h1>@ViewData["Title"]</h1></div>
</div>